{
    "name": "PHPBoost",
    "id": "phpboost",
    "packaging_format": 1,
    "description": {
        "en": "PHPBoost package for YunoHost.",
        "fr": "PHPBoost pour YunoHost."
    },
    "version": "5.2",
    "url": "https://www.phpboost.com/",
    "license": "GPL",
    "maintainer": {
        "name": "liberodark",
        "email": "liberodark@gmail.com"
    },
    "requirements": {
        "yunohost": ">= 4.2.4"
    },
    "multi_instance": false,
    "services": [
        "nginx",
        "php7.3-fpm"
    ],
    "arguments": {
        "install": [{
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/phpboost",
                "default": "/phpboost"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": "true"
            }
        ]
    }
}
